
	public class ExtractSkyCounts {

	public static void main(String[] args) {
		
		String string1 = "Insert row 1 from AiJ Measurements Table";
		String string2 = "Insert row 2 from AiJ Measurements Table";
		String[] parts1 = string1.split("	");
		String[] parts2 = string2.split("	");
		
		
		for (int i = 0; i < parts1.length; i++) {
			if(parts1[i].matches("Source-Sky.*")){
				System.out.println(parts1[i] + "	" + parts2[i]);
				}
		}

		    }

		}
	
